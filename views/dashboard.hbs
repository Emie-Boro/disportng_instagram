<h2>{{{name}}}</h2>
<p>{{{username}}}</p>
<form action="/export" method="post" enctype="multipart/form-data">
    <div class="image-div">
        <p>image</p>
        <input type="file" name="image" id="">
    </div>
    <div>
        <p>Title</p>
        <input type="text" class="title-input" name="title">
    </div>
    <div>
        <p>Body</p>
        <textarea name="content" id="" cols="30" rows="10" class="txtA"></textarea>
    </div>
    <input type="submit" value="submit" class="btn">
</form>
